{
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"module": "CommonJS",
		"moduleResolution": "node",
		"outDir": "out",
		"rootDir": "src",
		"lib": ["ES2020"],
		"target": "ES2020",
		"types": ["node", "vscode"],
		"esModuleInterop": true,
		"skipLibCheck": true,
		"noEmit": false,
		"allowImportingTsExtensions": false,
		"verbatimModuleSyntax": false
	},
	"include": ["src"]
}
